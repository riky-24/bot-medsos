{
     "env": {
          "node": true,
          "es2021": true
     },
     "extends": "eslint:recommended",
     "parserOptions": {
          "ecmaVersion": "latest",
          "sourceType": "module"
     },
     "rules": {
          "no-console": "warn",
          "max-lines": [
               "warn",
               400
          ],
          "max-lines-per-function": [
               "warn",
               50
          ],
          "complexity": [
               "warn",
               10
          ],
          "no-unused-vars": "warn"
     }
}